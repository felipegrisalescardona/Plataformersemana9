    }
    vel_horizontal = 0;
}
x +=vel_horizontal;

//Colisión verticalonga
if (place_meeting(x,y+vel_vertical,obj_block))
{
    while(!place_meeting(x,y+sign(vel_vertical),obj_block))
    {
        y += sign(vel_vertical);
    }
    vel_vertical = 0;
}
y +=vel_vertical;


</string>
